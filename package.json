{
  "name": "eslint-prettier",
  "version": "1.0.0",
  "description": "",
  "main": "text.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier src/*.js --write",
    "start": "node .",
    "lint": "eslint src/*.js",
    "lint:fix": "eslint src/*.js --fix",
    "lint:fix && format": "eslint src/*.js --fix && format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torumanabe/ESLInt-Prettier.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/torumanabe/ESLInt-Prettier/issues"
  },
  "homepage": "https://github.com/torumanabe/ESLInt-Prettier#readme",
  "devDependencies": {
    "eslint": "^8.23.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1"
  },
  "dependencies": {
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "mrm": "^0.1.2",
    "prettier": "^2.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint:fix",
      "format"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "printWidth": 40,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  }
}
